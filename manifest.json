{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "https://gitcuss.herokuapp.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.github.com/*", "https://github.com/*/*"],
      "css": ["style.css"],
      "js": ["jquery.js","app.js", "handlebars.js"]
    }
  ],
  "browser_action": {
  },
  "web_accessible_resources": [
    "templates/*"
  ]
  
}